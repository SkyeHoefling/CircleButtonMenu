<?xml version="1.0"?>
<package>
  <metadata>
    <id>CircleButtonMenu</id>
    <version>$version$</version>
    <title>Circle Button Menu control for Xamarin.Forms</title>
    <authors>Andrew Hoefling</authors>
    <owners>Andrew Hoefling</owners>
    <licenseUrl>https://github.com/ahoefling/CircleButtonMenu/blob/master/LICENSE</licenseUrl>
